    "redis": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.4.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
